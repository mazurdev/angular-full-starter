{
  "name": "angular-full-starter",
  "version": "2.0.0",
  "description": "Angular starter, which combines all the necessary components for writing a complete project",
  "keywords": [
    "Angular",
    "Full Starter",
    "TypeScript",
    "PWA",
    "SSR + Prerender",
    "Service worker",
    "Lazy loading",
    "SEO"
  ],
  "repository": "github:mazurdev/angular-full-starter",
  "contributors": [
    "Vladislav Mazur <vlad.developm@gmail.com> (https://mazurdev.com)"
  ],
  "author": "Vladislav Mazur <vlad.developm@gmail.com> (https://mazurdev.com)",
  "homepage": "https://example.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ngx-unused-css || node minifier-json.js && ng serve --port=4204",
    "serve:dist": "cd dist/client && http-server -o",
    "serve:ssr": "node dist/server/main.js",
    "serve:ssr-debugg": "ng run angular-full-starter:serve-ssr",
    "serve:ssr-staging-debugg": "ng run angular-full-starter:serve-ssr:staging",
    "build:ssr": "npm run build:prod && ng run angular-full-starter:server:production",
    "build:ssr-staging": "npm run build:prod-staging && ng run angular-full-starter:server:staging",
    "build:prerender": "ng run angular-full-starter:prerender:production --routesFile routes.txt && node minifier-index.js",
    "build:prerender-staging": "ng run angular-full-starter:prerender:staging --routesFile routes.txt && node minifier-index.js",
    "build:prod": "node minifier-json.js && ng build --prod --stats-json",
    "build:prod-staging": "node minifier-json.js && ng build --prod --stats-json --configuration staging",
    "analyze:bundle": "webpack-bundle-analyzer dist/client/stats-es2015.json",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/platform-server": "~9.0.7",
    "@angular/pwa": "^0.900.7",
    "@angular/router": "~9.0.7",
    "@angular/service-worker": "~9.0.7",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-toolkit/pwa": "^8.0.4",
    "@ng-toolkit/universal": "^8.1.0",
    "@nguniversal/common": "^9.0.2",
    "@nguniversal/express-engine": "^9.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-lite/json-ld": "^0.6.2",
    "aos": "^2.3.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.4",
    "domino": "^2.1.4",
    "express": "^4.17.1",
    "modern-normalize": "^0.6.0",
    "ngx-device-detector": "^1.4.1",
    "rfs": "^9.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@nguniversal/builders": "^9.0.2",
    "@types/estree": "0.0.44",
    "@types/express": "^4.17.3",
    "@types/node": "~12.12.31",
    "codelyzer": "^5.2.1",
    "html-minifier": "^4.0.0",
    "http-server": "^0.12.1",
    "jsonc": "^2.0.0",
    "ngx-unused-css": "^1.2.1",
    "ts-loader": "^9.4.2",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^5.0.1"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0",
    "yarn": ">= 1.0.0"
  }
}
