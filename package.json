{
  "name": "angular-full-starter",
  "version": "1.0.0",
  "description": "Angular starter, which combines all the necessary components for writing a complete project",
  "keywords": [
    "Angular"
  ],
  "repository": "github:mazurdev/angular-full-starter",
  "contributors": [
    "Vladislav Mazur <vlad.developm@gmail.com> (https://mazurdev.com)"
  ],
  "author": "Vladislav Mazur <vlad.developm@gmail.com> (https://mazurdev.com)",
  "homepage": "https://example.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4204",
    "serve:ssr": "node dist/server",
    "serve:prerender": "cd dist/browser && http-server",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender && node index-minifier.js",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "npm run build:prod && ng run angular-full-starter:server:production",
    "generate:prerender": "cd dist && node prerender",
    "build:stats": "ng build --prod --stats-json",
    "build:analyze-bundle": "webpack-bundle-analyzer dist/browser/stats-es2015.json",
    "check:https": "cd dist/browser && http-server -o"
  },
  "dependencies": {
    "@angular/animations": "^8.2.4",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/platform-server": "~8.2.4",
    "@angular/pwa": "^0.800.2",
    "@angular/router": "~8.2.4",
    "@angular/service-worker": "~8.2.4",
    "@ng-toolkit/pwa": "^7.1.2",
    "@ng-toolkit/universal": "^7.1.2",
    "@nguniversal/common": "^8.1.1",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "@ngx-lite/json-ld": "^0.6.2",
    "classlist.js": "^1.1.20150312",
    "domino": "^2.1.3",
    "express": "^4.15.2",
    "ngx-cookie-service": "^2.2.0",
    "ngx-device-detector": "^1.3.6",
    "rfs": "^8.0.4",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/express": "^4.17.1",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "html-minifier": "^4.0.0",
    "http-server": "^0.11.1",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.1.0"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0",
    "yarn": ">= 1.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not ie <= 8"
  ]
}
